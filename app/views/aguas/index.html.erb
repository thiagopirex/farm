<p id="notice"><%= notice %></p>

<h1>Listing Aguas</h1>

<table>
  <thead>
    <tr>
      <th>Tipo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @aguas.each do |agua| %>
      <tr>
        <td><%= agua.tipo %></td>
        <td><%= link_to 'Show', agua %></td>
        <td><%= link_to 'Edit', edit_agua_path(agua) %></td>
        <td><%= link_to 'Destroy', agua, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Agua', new_agua_path %>
